{
  "expo": {
    "name": "rookies-fantasy",
    "slug": "rookies-fantasy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "fonts": [
      {
        "asset": "./assets/fonts/ClashDisplay-Bold.ttf",
        "family": "ClashDisplay-Bold"
      },
      {
        "asset": "./assets/fonts/Manrope-Regular.otf",
        "family": "Manrope-Regular"
      }
    ],
    "splash": {
      "image": "",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info-development.plist",
      "bundleIdentifier": "com.rookies.rookiesfantasy.development",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": ""
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
